{
  "name": "tomato-app",
  "description": "变得更好",
  "version": "0.0.1",
  "main": "main.js",
  "productName": "番茄钟",
  "scripts": {
    "start": "electron .",
    "pack-win": "electron-builder --win --x64",
    "pack-mac": "electron-builder --mac --x64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.4"
  },
  "author": {
    "name": "maxilo",
    "email": "2802146520@qq.com"
  },
  "build": {
    "appId": "com.maxilo.app.tomato",
    "productName": "番茄钟",
    "asar": false,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "../dist"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "../common/favicon.ico"
    }
  }
}
